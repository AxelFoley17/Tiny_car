# Tool invocations
LPC845_Project_UH_tavmero.axf: $(OBJS) $(USER_OBJS) makefile $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map="LPC845_Project_UH_tavmero.map" -Xlinker --gc-sections -Xlinker -print-memory-usage -Xlinker --sort-section=alignment -Xlinker --cref -mcpu=cortex-m0plus -mthumb -T LPC845_Project_UH_tavmero_Debug.ld -o "LPC845_Project_UH_tavmero.axf" $(OBJS) $(USER_OBJS) $(LIBS)
